  },
  "homepage": "https://yourusername.github.io/vnli-prototype",
  "devDependencies": {
    "gh-pages": "^6.0.0",
    "serve": "^14.2.0",
    "eslint": "^8.50.0",
    "stylelint": "^15.11.0",
    "prettier": "^3.0.0"
  },
  "browserslist": [
    "> 1%",
